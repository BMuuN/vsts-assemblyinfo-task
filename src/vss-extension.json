{
    "manifestVersion": 1,
    "id": "Assembly-Info-Task",
    "name": "Assembly Info",
    "public": true,
    "version": "1.0.0",
    "publisher": "bleddynrichards",
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "description": "Set assembly information such as Version, Copyright and Trademark etc from Team Foundation Server / Team Services build.",
    "icons": {
        "default": "images/Logo128x128.png"
    },
    "categories": [
        "Build and release"
    ],
    "tags": [
        "Build",
        "Release",
        "Binary",
        "Assembly",
        "Version",
        "Continuous Delivery",
        "Continuous Integration",
        "Management",
        "Utility",
        "Tasks"
    ],
    "screenshots": [
        {
            "path": "images/Task_List.png"
        }
    ],
    "links": {
        "learn": {
            "uri": "https://github.com/BMuuN/vsts-assemblyinfo-task/wiki"
        },
        "support": { 
            "uri": "https://github.com/BMuuN/vsts-assemblyinfo-task/issues"
        },
        "license": {
            "uri": "https://github.com/BMuuN/vsts-assemblyinfo-task/blob/master/LICENSE"
        }
    },
    "repository": {
        "type": "git",
        "uri": "https://github.com/BMuuN/vsts-assemblyinfo-task"
    },
    "badges": [
        {
            "href": "https://github.com/BMuuN/vsts-assemblyinfo-task",
            "uri": "https://bmuun.visualstudio.com/_apis/public/build/definitions/86c93e13-9469-4df8-95f0-98c43c760a09/8/badge",
            "description": "Team Services build for the project"
        }
    ],
    "content": {
        "details": {
            "path": "Overview.md"
        },
        "license": {
            "path": "License.md"
        }
    },
    "branding": {
        "color": "#222730",
        "theme": "dark"
    },
    "files": [
        {
            "path": "Common"
        },
        {
            "path": "NetFramework"
        },
        {
            "path": "NetCore"
        },
        {
            "path": "images",
            "addressable": true
        }
    ],
    "contributions": [
        {
            "id": "Assembly-Info-Task",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "task"
            }
        }
    ]
}