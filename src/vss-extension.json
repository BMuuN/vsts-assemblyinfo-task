{
    "manifestVersion": 1,
    "id": "Assembly-Info-Task",
    "name": "Assembly Info",
    "public": true,
    "version": "1.0.0",
    "publisher": "bleddynrichards",
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "description": "Set assembly information such as Version, Copyright and Trademark etc from Azure DevOps build.",
    "icons": {
        "default": "images/Logo128x128.png"
    },
    "categories": [
        "Azure Pipelines"
    ],
    "tags": [
        "Build",
        "Release",
        "Binary",
        "Assembly",
        "Version",
        "Versioning",
        "Continuous Delivery",
        "Continuous Integration",
        "Management",
        "Utility",
        "Tasks"
    ],
    "screenshots": [
        {
            "path": "images/Task_List_Net_Core.png"
        },
        {
            "path": "images/Task_List_Net_Framework.png"
        }
    ],
    "links": {
        "learn": {
            "uri": "https://github.com/BMuuN/vsts-assemblyinfo-task/wiki"
        },
        "support": { 
            "uri": "https://github.com/BMuuN/vsts-assemblyinfo-task/issues"
        },
        "privacypolicy": {
            "uri": "https://github.com/BMuuN/vsts-assemblyinfo-task/blob/master/src/Privacy.md"
        }
    },
    "repository": {
        "type": "git",
        "uri": "https://github.com/BMuuN/vsts-assemblyinfo-task"
    },
    "badges": [
        {
            "href": "https://github.com/BMuuN/vsts-assemblyinfo-task",
            "uri": "https://bmuun.visualstudio.com/_apis/public/build/definitions/86c93e13-9469-4df8-95f0-98c43c760a09/8/badge?branchName=master",
            "description": "Azure DevOps Build"
        },
        {
            "href": "https://github.com/BMuuN/vsts-assemblyinfo-task",
            "uri": "https://bmuun.vsrm.visualstudio.com/_apis/public/Release/badge/86c93e13-9469-4df8-95f0-98c43c760a09/1/2",
            "description": "Azure DevOps Release"
        }
    ],
    "content": {
        "details": {
            "path": "Overview.md"
        },
        "license": {
            "path": "License.md"
        }
    },
    "branding": {
        "color": "#222730",
        "theme": "dark"
    },
    "files": [
        {
            "path": "netframework"
        },
        {
            "path": "netcore"
        },
        {
            "path": "images",
            "addressable": true
        }
    ],
    "contributions": [
        {
            "id": "Assembly-Info-Task",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "netframework"
            }
        },
        {
            "id": "Assembly-Info-NetCore",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "netcore"
            }
        }
    ]
}