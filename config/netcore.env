# Encoding
INPUT_FILEENCODING="auto"
INPUT_WRITEBOM=false

# Versioning
INPUT_PACKAGEVERSION="9.8.7-beta65"
INPUT_VERSIONNUMBER="TS Extension Test Build_2018.11.*"
INPUT_FILEVERSIONNUMBER="1990.03.*.*"
