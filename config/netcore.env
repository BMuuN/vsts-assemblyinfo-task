NODE_ENV="development"
SYSTEM_DEBUG="true"
TASK_TEST_TRACE=1

# Global
INPUT_INSERTATTRIBUTES=true
INPUT_PATH="C:\DEV\GIT\vsts-assemblyinfo-task\tests\projects"
#INPUT_FILENAMES="**/IDontExist.csproj"
INPUT_FILENAMES="**/*.csproj,**/*.vbproj"
